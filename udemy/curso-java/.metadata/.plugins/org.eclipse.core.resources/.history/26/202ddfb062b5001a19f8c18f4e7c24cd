package classe;


public class ProdutoTeste {
	public static void main(String[] args) {
		
		Produto p1 = new Produto();
		var p2 = new Produto();
		p1.nome = "Notebook";
		p1.preco = 2500.00;
		p1.desconto = 0.25;

		p2.nome = "SmartPhone	";
		p2.preco = 3650.25;
		p2.desconto = 0.10;
		
		System.out.println(p1.nome + "" + p2.nome);
	}
}
